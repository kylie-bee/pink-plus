{
	"$schema": "vscode://schemas/color-theme",
	"name": "Pink+",
	"colors": {
		"checkbox.border": "#6B6B6B",
		// "editor.background": "#2b2631",
		"editor.background": "#201e23",
		"editor.foreground": "#D4D4D4", // #foreground
		"editor.inactiveSelectionBackground": "#3A3D41", // #inactive-background
		"editorIndentGuide.background1": "#404040",
		"editorIndentGuide.activeBackground1": "#707070",
		"editor.selectionHighlightBackground": "#ADD6FF26",
		"peekView.border": "#ab0eb3", // #peekview-border
		"peekViewEditor.background": "#230b31", // #peekview-background
		"peekViewEditorGutter.background": "#230b31", // #peekview-background
		"peekViewEditor.matchHighlightBackground": "#ff00d94d",
		"peekViewResult.background": "#2d2a32", // #sidebar-background
		"peekViewResult.matchHighlightBackground": "#ff00d94d",
		"peekViewResult.selectionBackground": "#2e0f41",
		"peekViewResult.selectionForeground": "#ffffff",
		"peekViewResult.fileForeground": "#ffffff",
		"peekViewResult.lineForeground": "#ffffff",
		"peekViewTitle.background": "#4c0e73",
		"peekViewTitleDescription.foreground": "#ffffff",
		"debugExceptionWidget.background": "#230b31", // #peekview-background
		"debugExceptionWidget.border": "#ab0eb3", // #peekview-border
		"list.dropBackground": "#383B3D",
		"activityBar.background": "#3b373c", // #menu-background
		"activityBar.foreground": "#ffb2fa", // #menu-foreground
		"activityBarBadge.background": "#d316c7", // #button-background
		"button.background": "#d316c7", // #button-background
		"sideBarTitle.foreground": "#BBBBBB",
		"sideBar.background": "#2d2a32", // #sidebar-background
		"input.placeholderForeground": "#A6A6A6",
		"menu.separatorBackground": "#454545", // #menu-border
		"menu.border": "#454545", // #menu-border
		"editorGroupHeader.tabsBackground": "#28252c", // #group-header-background
		"editorGroupHeader.noTabsBackground": "#28252c", // #group-header-background
		"tab.inactiveBackground": "#37343d",
		"statusBar.background": "#d316c7", // #button-background
		"statusBar.debuggingBackground": "#008243",
		"statusBarItem.remoteForeground": "#FFF", // #selected-foreground
		"statusBarItem.remoteBackground": "#4c0e73",
		"ports.iconRunningProcessForeground": "#943258",
		"sideBarSectionHeader.background": "#0000",
		"sideBarSectionHeader.border": "#ccc3", // #sidebar-border
		"tab.selectedBackground": "#222222",
		"tab.selectedForeground": "#ffffffa0",
		"tab.lastPinnedBorder": "#ccc3", // #sidebar-border
		"list.activeSelectionIconForeground": "#FFF", // #selected-foreground
		"terminal.inactiveSelectionBackground": "#3A3D41", // #inactive-background
		"widget.border": "#303031",
		"actionBar.toggledBackground": "#383a49",
		"notificationCenterHeader.background": "#4c0e73",
		"notifications.background": "#2d2a32", // #sidebar-background
		"textLink.foreground": "#db18ce", // #button-background
		"textLink.activeForeground": "#e33ad8",
		// bracket matching
		"editorBracketHighlight.foreground1": "#00ff9d",
		"editorBracketHighlight.foreground2": "#00eeff",
		"editorBracketHighlight.foreground3": "#0080ff",
		"editorBracketHighlight.foreground4": "#1100ff",
		"editorBracketHighlight.foreground5": "#7b00ff",
		"editorBracketHighlight.foreground6": "#ff00d9",
	},
	"tokenColors": [
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
			],
			"settings": {
				"foreground": "#99de5e" // #function
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#4ec9b4" // #type
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#4ec9b4" // #type
			}
		},
		{
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.control",
				"source.cpp keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.directive.using",
				"keyword.other.operator",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#915aeb" // #control
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
				"constant.other.placeholder", // placeholders in strings
			],
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"name": "Constants and enums",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#ff3fe2" // #constants
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"name": "Regular expression groups",
			"scope": [
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": [
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp"
			],
			"settings": {
				"foreground": "#d8ac4d" // #regexp
			}
		},
		{
			"scope": [
				"keyword.operator.or.regexp",
				"keyword.control.anchor.regexp"
			],
			"settings": {
				"foreground": "#99de5e" // #function
			}
		},
		{
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#e77ecd" // #regexp-operator
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other.option"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#e77ecd" // #regexp-operator
			}
		},
		{
			"scope": "entity.name.label",
			"settings": {
				"foreground": "#C8C8C8" // #label
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"string meta.image.inline.markdown",
				"variable.legacy.builtin.python"
			],
			"settings": {
				"foreground": "#D4D4D4" // #foreground
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#687469", // #comment
				"fontStyle": "italic"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#915aeb" // #control
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#cea8c1" // #number-literal
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#e77ecd" // #regexp-operator
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#e77ecd" // #regexp-operator
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747" // #invalid
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#cea8c1" // #number-literal
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#687469" // #comment
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#67e6be" // #markdown-list-prepend
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#808080" // #xml-brackets
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#cea8c1" // #number-literal
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#915aeb" // #control
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#915aeb" // #control
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#d8b15c" // #string
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#d8ac4d" // #regexp
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#d4d4d4" // #foreground
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#d4d4d4" // #foreground
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#cea8c1" // #number-literal
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#ff92ef" // #variable
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#cea8c1" // #number-literal
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#d4d4d4" // #foreground
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#c65584" // #constant-other
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator": "#915aeb", // #control
		"stringLiteral": "#d8b15c", // #string
		"customLiteral": "#99de5e", // #function
		"numberLiteral": "#cea8c1", // #number-literal
	}
}